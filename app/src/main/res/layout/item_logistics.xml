<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:layout_marginTop="10dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="40dp"
        android:layout_width="match_parent"
        android:background="@drawable/qifyshang"
        android:layout_height="70dp"
        android:id="@+id/rl_up">
        <TextView
            android:layout_width="wrap_content"
            android:text="支付成功"
            android:textColor="#434343"
            android:textSize="18sp"
            android:layout_marginTop="5dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="66dp"
            android:layout_marginStart="66dp"
            android:id="@+id/textView9" />
        <TextView
            android:layout_width="wrap_content"
            android:textColor="#434343"
            android:textSize="18sp"
            android:layout_marginTop="5dp"
            android:text="2016-02-12  16:40"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView9"
            android:layout_alignLeft="@+id/textView9"
            android:layout_alignStart="@+id/textView9" />
    </RelativeLayout>
    <RelativeLayout
       
        android:layout_marginLeft="20dp"
        android:layout_marginRight="40dp"
        android:layout_width="match_parent"
        android:background="@drawable/qifyzhongj"
        android:layout_height="70dp"
        android:id="@+id/rl_middle">
        <TextView
            android:layout_width="wrap_content"
            android:text="支付成功"
            android:textColor="#434343"
            android:textSize="18sp"
            android:layout_marginTop="5dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="66dp"
            android:layout_marginStart="66dp"
            android:id="@+id/tv_zjmessage" />
        <TextView
            android:layout_width="wrap_content"
            android:textColor="#434343"
            android:textSize="18sp"
            android:layout_marginTop="5dp"
            android:text="2016-02-12  16:40"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_zjmessage"
            android:layout_alignLeft="@+id/tv_zjmessage"
            android:layout_alignStart="@+id/tv_zjmessage" />
    </RelativeLayout>
    <RelativeLayout
        
        android:layout_marginLeft="20dp"
        android:layout_marginRight="40dp"
        android:layout_width="match_parent"
        android:background="@drawable/qifydibu"
        android:layout_height="70dp"
        android:id="@+id/rl_bottom">
        <TextView
            android:layout_width="wrap_content"
            android:text="支付成功"
            android:textColor="#434343"
            android:textSize="18sp"
            android:layout_marginTop="20dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="66dp"
            android:layout_marginStart="66dp"
            android:id="@+id/tv_endmessage" />
        <TextView
            android:layout_width="wrap_content"
            android:textColor="#434343"
            android:textSize="18sp"
            android:layout_marginTop="5dp"
            android:text="2016-02-12  16:40"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_endmessage"
            android:layout_alignLeft="@+id/tv_endmessage"
            android:layout_alignStart="@+id/tv_endmessage" />
    </RelativeLayout>



</LinearLayout>